{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@hooks/*": ["./hooks/*"],
      "@services/*": ["./services/*"],
      "@contsnts/*": ["./constants/*"],
      "@components/*": ["./components/*"],
      "@constants/*": ["./constants/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

