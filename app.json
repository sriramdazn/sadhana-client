{
  "expo": {
    "name": "sadhana-client",
    "slug": "sadhana-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/yoga.png",
    "scheme": "clientreactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/yoga.png",
        "backgroundImage": "./assets/yoga.png",
        "monochromeImage": "./assets/yoga.png"
 
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jyotsnaanand5.sadhanaclient"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/yoga.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "14b20cfe-6f69-44c2-8f21-fdb8f7589d15"
      }
    }
  }
}
